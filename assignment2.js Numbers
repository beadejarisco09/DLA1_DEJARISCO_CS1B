// Task 4: Booleans

// Creating variables
let isJsProgrammingLanguage = true;
let isJsHard = false;
let favNumb = 9;
let favNumbUndefined = undefined;

// Logging variables to the console
console.log("isJsProgrammingLanguage:", isJsProgrammingLanguage);
console.log("isJsHard:", isJsHard);
console.log("favNumb:", favNumb);
console.log("favNumbUndefined:", favNumbUndefined);
